{
  "name": "language-learning-backend",
  "version": "1.0.0",
  "description": "AWS SAM backend for language learning app",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "sam build",
    "setup-secrets:dev": "./scripts/setup-secrets.sh dev",
    "setup-secrets:prod": "./scripts/setup-secrets.sh prod",
    "deploy:dev": "./scripts/deploy-dev.sh",
    "deploy:prod": "./scripts/deploy-prod.sh",
    "logs:dev": "sam logs -n GetWordsFunction --stack-name language-learning-api-dev --tail --region us-west-2",
    "logs:prod": "sam logs -n GetWordsFunction --stack-name language-learning-api-prod --tail --region ap-east-1",
    "delete:dev": "sam delete --stack-name language-learning-api-dev --no-prompts --region us-west-2",
    "delete:prod": "sam delete --stack-name language-learning-api-prod --region ap-east-1",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@aws-sdk/client-dynamodb": "^3.709.0",
    "@aws-sdk/lib-dynamodb": "^3.709.0",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.0.5"
  },
  "devDependencies": {}
}
